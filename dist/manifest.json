{
  "manifest_version": 3,
  "name": "Guardium_Tdoc",
  "description": "Password manager",
  "version": "1.0",

  "action": {
    "default_popup": "index.html",
    "default_title": "Open Guardium"
  },

  "icons": {
    "16": "logo192.png",
    "48": "logo192.png",
    "128": "logo192.png"
  }, 

  "permissions": ["activeTab", "tabs", "storage","scripting"],
  "host_permissions": ["<all_urls>"]
}
